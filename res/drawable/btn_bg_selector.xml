<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<item android:state_pressed="true" >
		<shape android:shape="oval">
			<solid
			    android:color="#29000000" />
			<padding
			    android:left="5dp"
			    android:top="5dp"
			    android:right="0dp"
			    android:bottom="5dp" />
		</shape>    
	</item>
	
	<item>
		<shape android:shape="oval">
			<solid
			    android:color="#00000000" />
			<padding
			    android:left="5dp"
			    android:top="5dp"
			    android:right="0dp"
			    android:bottom="5dp" />
		</shape>    
	</item>	
	
</selector>